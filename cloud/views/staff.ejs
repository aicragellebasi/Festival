<!doctype html>
<html lang="en">
<head>
<% include metas %>
<title>Primavera de los Realizadores</title> 
</head>

<body id="reserve">

<% include nav %>

<div class="container">
	<header>
		<div id="logoImg">
            <a href="#">
                <img src="/img/logo.png">
            </a>
        </div>
        <div id="logoTxt">
                <h1><a href="#">Primavera de los<br>Realizadores</a></h1>
                <h4>New York City</h4>
        </div>
	</header>
	<section id="featured">
		<h2>Our Instructors, Speakers and Filmmakers</h2>
		<p>We are proud to feature our Staff Members, all professional of the film industry and related to Spanish movies somehow...</p>
		<div id="leadImg">
			<img src="/img/mainImg-01.png">
		</div>
	</section>
	<section id="main">

		<% for (var i=0; i<staffs.length; i++){ %>
		<% var staff = staffs[i] %>
		<h4><%= staff.get('fname') %> <%= staff.get('lname') %></h4>
		<p><%= staff.get('bio') %></p>
		<a href="/staff/<%= staff.id %>" class="button">View Full Bio</a>
		<% } %>
	</section>
	<footer><% include footer %></footer>
</div>

