<body id="reserve">

<% include nav %>

<div class="container">
	<header>
            <a href="/">
                <img src="/img/logo.png" id="logo">
            </a>
                <h1><a href="index.html">Primavera de los Realizadores</a></h1>
				<h2>Our Instructors, Speakers and Filmmakers</h2>
                <img src="/img/mainImg-01.png" id="perroAndalous">
	</header>
	<section id="main">

		<% for (var i=0; i<staffs.length; i++){ %>
		<% var staff = staffs[i] %>
		<h4><%= staff.get('fname') %> <%= staff.get('lname') %></h4>
		<p><%= staff.get('bio') %></p>
		<a href="/staff/<%= staff.id %>" class="button">View Full Bio</a>
		<% } %>
	</section>
	<footer><% include footer %></footer>
</div>

